{
	"name": "Ubuntu",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"postCreateCommand": "pip install -r requirements.txt",
	"containerEnv": {
		"PYRIGHT_PYTHON_PYLANCE_VERSION": "latest-release"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"charliermarsh.ruff",
				"tamasfe.even-better-toml"
			]
		}
	}
}
